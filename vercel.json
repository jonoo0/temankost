{
    "version": 2,
      "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.3.6" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "public/build/"

        },
        {
            "src": "/css/(.*)",
            "dest": "public/css/"

        },
        {
            "src": "/customer/(.*)",
            "dest": "public/customer/"

        },
        {
            "src": "/dist/(.*)",
            "dest": "public/dist/"

        },
        {
            "src": "/fe/(.*)",
            "dest": "public/fe/"

        },
        {
            "src": "/image-uploader-master/(.*)",
            "dest": "public/image-uploader-master/"

        },
        {
            "src": "/nota/(.*)",
            "dest": "public/nota/"

        },
        {
            "src": "/pages/(.*)",
            "dest": "public/pages/"

        },
        {
            "src": "/plugins/(.*)",
            "dest": "public/plugins/"

        },
        {
            "src": "/uploads/(.*)",
            "dest": "public/uploads/"

        },
        {
        "src": "/(.*)",
        "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://yourproductionurl.com",
 
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
 
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    } 
}